@model PagedList.IPagedList<ShoeStore.Models.Bitacora>
@using PagedList.Mvc;

@{
    ViewBag.Title = "Bitacora";
}

<h2>Acciones Realizadas</h2>
<br />
<br />

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().Accion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().Table)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().User)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstOrDefault().DateofInsert)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Accion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Table)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DateofInsert)
            </td>
            <td></td>
        </tr>
    }

</table>

Página: @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber), de: @Model.PageCount
@Html.PagedListPager(Model, page => Url.Action("Index", new
{
    page,
    sortOrder =
    ViewBag.CurrentSort,
    currentFilter = ViewBag.CurrentFilter
}))
